[user]
	name = Kevin Rocker
	email = kevin.j.rocker@gmail.com
[alias]
	co = checkout					# Checkout
	cob = checkout -b				# Checkout Branch
	ec = config --global -e				# Open Editor
	remove = remote					# Spelling
	wip = !git add -u && git commit -m "WIP" 	# Create Temporary Commit
	undo = reset HEAD~1 --mixed			# Remove prev. commit, keep changes
	amend = commit -a --amend			# Add further changes to prev. commit
